  "scripts": {
    "test": "gulp test"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "browser": true,
      "jquery": true,
      "es6": true
    },
    "globals": {
      "Vue": true,
      "VueRouter": true,
      "_": true,
      "Clipboard": true
    },
    "ecmaFeatures": {
      "arrowFunctions": true
    },
    "rules": {
      "quotes": [
        2,
        "double"
      ],
      "strict": [
        2,
        "function"
      ],
      "brace-style": 1,
      "camelcase": 1,
      "comma-spacing": 1,
      "comma-style": 1,
      "consistent-this": 1,
      "no-mixed-spaces-and-tabs": 1,
      "indent": [
        1,
        2
      ],
      "comma-dangle": 1,
      "eol-last": 1,
      "key-spacing": 1,
      "new-cap": 1,
      "new-parens": 1,
      "newline-after-var": 1,
      "no-trailing-spaces": 1,
      "space-before-blocks": 1,
      "space-in-parens": 1,
      "space-infix-ops": 1
    }
  },
  "dependencies": {}
